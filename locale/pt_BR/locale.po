msgid "plugins.generic.documentMetadataChecklist.displayName"
msgstr "Verificação de metadados do documento"

msgid "plugins.generic.documentMetadataChecklist.description"
msgstr "Este plugin verifica o conteúdo do documento submetido de acordo com os metadados da submissão"


msgid "plugins.generic.documentMetadataChecklist.status.title"
msgstr "Verificação do documento"

msgid "plugins.generic.documentMetadataChecklist.status.messageSuccess"
msgstr "O documento enviado está de acordo com as regras de submissão"

msgid "plugins.generic.documentMetadataChecklist.status.messageWarning"
msgstr "O documento enviado possui problemas na sua composição"

msgid "plugins.generic.documentMetadataChecklist.status.messageError"
msgstr "O documento enviado possui erros na sua composição"

msgid "plugins.generic.documentMetadataChecklist.status.authorsContributionSuccess"
msgstr "A declaração de contribuição dos autores foi identificada no documento"

msgid "plugins.generic.documentMetadataChecklist.status.authorsContributionError"
msgstr "A declaração de contribuição dos autores não foi identificada no documento"

msgid "plugins.generic.documentMetadataChecklist.status.orcidSuccess"
msgstr "Os ORCIDs de todos os autores foram identificados"

msgid "plugins.generic.documentMetadataChecklist.status.orcidWarning"
msgstr "Identificamos {$numOrcids} ORCIDs de um total de {$numAuthors} autores"

msgid "plugins.generic.documentMetadataChecklist.status.orcidError"
msgstr "Não foram identificados ORCIDs no documento"

msgid "plugins.generic.documentMetadataChecklist.status.conflictInterestSuccess"
msgstr "A declaração de conflito de interesses foi identificada no documento"

msgid "plugins.generic.documentMetadataChecklist.status.conflictInterestError"
msgstr "A declaração de conflito de interesses não foi identificada no documento"

msgid "plugins.generic.documentMetadataChecklist.status.metadataEnglishSuccess"
msgstr "Todos os metadados em inglês foram detectados no documento"

msgid "plugins.generic.documentMetadataChecklist.status.metadataEnglishWarning"
msgstr "Os seguintes metadados em inglês não foram detectados no documento: {$textoMetadados}"

msgid "plugins.generic.documentMetadataChecklist.status.metadataEnglishError"
msgstr "Nenhum metadado em inglês foi detectado no documento"

msgid "plugins.generic.documentMetadataChecklist.status.advice"
msgstr "Você pode corrigir esse problemas na revisão da submissão"
