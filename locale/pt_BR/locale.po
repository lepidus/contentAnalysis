msgid "plugins.generic.contentAnalysis.displayName"
msgstr "Análise de Conteúdo"

msgid "plugins.generic.contentAnalysis.description"
msgstr "Este plugin verifica o conteúdo do documento submetido de acordo com os metadados da submissão"

msgid "plugins.generic.contentAnalysis.status.title"
msgstr "Verificação do documento"

msgid "plugins.generic.contentAnalysis.status.messageSuccess"
msgstr "O documento enviado está de acordo com as regras de submissão"

msgid "plugins.generic.contentAnalysis.status.messageWarning"
msgstr "O documento enviado possui problemas na sua composição"

msgid "plugins.generic.contentAnalysis.status.messageError"
msgstr "O documento enviado possui erros na sua composição"

msgid "plugins.generic.contentAnalysis.status.authorsContributionSuccess"
msgstr "A declaração de contribuição dos autores foi identificada no documento"

msgid "plugins.generic.contentAnalysis.status.authorsContributionError"
msgstr "A declaração de contribuição dos autores não foi identificada no documento. Certifique-se que foi inserida uma seção com o nome \"Contribuição dos autores\" no documento."

msgid "plugins.generic.contentAnalysis.status.authorsContributionSkipped"
msgstr "A declaração de contribuição dos autores não é necessária em casos de autoria única"

msgid "plugins.generic.contentAnalysis.status.orcidSuccess"
msgstr "Os ORCIDs de todos os autores foram identificados"

msgid "plugins.generic.contentAnalysis.status.orcidWarning"
msgstr "Identificamos {$numOrcids} ORCIDs de um total de {$numAuthors} autores"

msgid "plugins.generic.contentAnalysis.status.orcidError"
msgstr ""
"Certifique-se de que todos os ORCID IDs foram inseridos no manuscrito no <a href=\"https://support.orcid.org/hc/en-us/articles/360006897674-Structure-of-the-ORCID-Identifier\">formato correto</a>. "
"Certifique-se também que os links correspondem ao registro ORCID correto de cada pessoa listada na autoria do manuscrito."

msgid "plugins.generic.contentAnalysis.status.conflictInterestSuccess"
msgstr "A declaração de conflito de interesses foi identificada no documento"

msgid "plugins.generic.contentAnalysis.status.conflictInterestError"
msgstr "A declaração de conflito de interesses não foi identificada no documento"

msgid "plugins.generic.contentAnalysis.status.metadataEnglishSuccess"
msgstr "Todos os metadados em inglês foram detectados no documento"

msgid "plugins.generic.contentAnalysis.status.metadataEnglishWarning"
msgstr "Os seguintes metadados em inglês não foram detectados no documento: {$textMetadata}"

msgid "plugins.generic.contentAnalysis.status.metadataEnglishError"
msgstr "Título, resumo e palavras-chave em inglês não foram detectados no documento"

msgid "plugins.generic.contentAnalysis.status.advice"
msgstr "Você pode corrigir esse problemas na revisão da submissão"
