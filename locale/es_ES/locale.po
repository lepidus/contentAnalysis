msgid "plugins.generic.documentMetadataChecklist.displayName"
msgstr "Verificación de metadatos del documento"

msgid "plugins.generic.documentMetadataChecklist.description"
msgstr "Este complemento verifica el contenido del documento enviado de acuerdo con los metadatos del envío"


msgid "plugins.generic.documentMetadataChecklist.status.title"
msgstr "Verificación del documento"

msgid "plugins.generic.documentMetadataChecklist.status.messageSuccess"
msgstr "El documento enviado cumple con las reglas de envío"

msgid "plugins.generic.documentMetadataChecklist.status.messageWarning"
msgstr "El documento enviado tiene problemas en su composición"

msgid "plugins.generic.documentMetadataChecklist.status.messageError"
msgstr "El documento enviado tiene errores en su composición"

msgid "plugins.generic.documentMetadataChecklist.status.authorsContributionSuccess"
msgstr "La declaración de contribución de los autores fue identificada en el documento"

msgid "plugins.generic.documentMetadataChecklist.status.authorContributionError"
msgstr "La declaración de contribución de los autores no fue identificada en el documento"

msgid "plugins.generic.documentMetadataChecklist.status.orcidSuccess"
msgstr "Se identificaron los ORCID de todos los autores"

msgid "plugins.generic.documentMetadataChecklist.status.orcidWarning"
msgstr "Identificamos los ORCIDs de solo {$numOrcids} autores"

msgid "plugins.generic.documentMetadataChecklist.status.orcidError"
msgstr "No se identificaron ORCIDs en el documento"

msgid "plugins.generic.documentMetadataChecklist.status.conflictInterestSuccess"
msgstr "La declaración de conflicto de intereses fue identificada en el documento"

msgid "plugins.generic.documentMetadataChecklist.status.conflictInterestError"
msgstr "La declaración de conflicto de intereses no fue identificada en el documento"

msgid "plugins.generic.documentMetadataChecklist.status.advice"
msgstr "Puede corregir estos problemas mediante la revisión del envío"
