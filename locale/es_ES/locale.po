msgid "plugins.generic.contentAnalysis.displayName"
msgstr "Análisis del contenido"

msgid "plugins.generic.contentAnalysis.description"
msgstr "Este complemento verifica el contenido del documento enviado de acuerdo con los metadatos del envío"


msgid "plugins.generic.contentAnalysis.status.title"
msgstr "Verificación del documento"

msgid "plugins.generic.contentAnalysis.status.messageSuccess"
msgstr "El documento enviado cumple con las reglas de envío"

msgid "plugins.generic.contentAnalysis.status.messageWarning"
msgstr "El documento enviado tiene problemas en su composición"

msgid "plugins.generic.contentAnalysis.status.messageError"
msgstr "El documento enviado tiene errores en su composición"

msgid "plugins.generic.contentAnalysis.status.authorsContributionSuccess"
msgstr "La declaración de contribución de los autores fue identificada en el documento"

msgid "plugins.generic.contentAnalysis.status.authorsContributionError"
msgstr "La declaración de contribución de los autores no fue identificada en el documento. Asegúrese de que haya una sección denominada \"Contribución de los autores\" en el documento."

msgid "plugins.generic.contentAnalysis.status.authorsContributionSkipped"
msgstr "La declaración de contribución de los autores no se requiere en los casos de autoría única."

msgid "plugins.generic.contentAnalysis.status.orcidSuccess"
msgstr "Se identificaron los ORCID de todos los autores"

msgid "plugins.generic.contentAnalysis.status.orcidWarning"
msgstr "Identificamos {$numOrcids} ORCIDs de un total de {$numAuthors} autores"

msgid "plugins.generic.contentAnalysis.status.orcidError"
msgstr ""
"Asegúrese de que todas las ID de ORCID se hayan ingresado en el manuscrito en el <a href=\"https://support.orcid.org/hc/en-us/articles/360006897674-Structure-of-the-ORCID-Identifier\">formato correcto</a>. "
"También asegúrese de que los enlaces coincidan con el registro ORCID correcto para cada persona que figura en la autoría del manuscrito."

msgid "plugins.generic.contentAnalysis.status.conflictInterestSuccess"
msgstr "La declaración de conflicto de intereses fue identificada en el documento"

msgid "plugins.generic.contentAnalysis.status.conflictInterestError"
msgstr "La declaración de conflicto de intereses no fue identificada en el documento"

msgid "plugins.generic.contentAnalysis.status.metadataEnglishSuccess"
msgstr "Se detectaron todos los metadatos en inglés en el documento"

msgid "plugins.generic.contentAnalysis.status.metadataEnglishWarning"
msgstr "Los siguientes metadatos en inglés no se detectaron en el documento: {$textMetadata}"

msgid "plugins.generic.contentAnalysis.status.metadataEnglishError"
msgstr "No se detectaron título, resumen y palabras-clave en inglés en el documento"

msgid "plugins.generic.contentAnalysis.status.advice"
msgstr "Puede corregir estos problemas mediante la revisión del envío"
